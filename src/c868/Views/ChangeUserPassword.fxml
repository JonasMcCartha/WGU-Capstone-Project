<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="360.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c868.Controllers.ChangeUserPasswordController">
 <children>
      <Label fx:id="PartsPageLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="21.0" prefHeight="30.0" prefWidth="300.0" text="Change Password" textAlignment="CENTER">
         <font>
            <Font name="Cambria Math" size="24.0" />
         </font>
      </Label>
      <AnchorPane fx:id="PasswordField" layoutX="30.0" layoutY="75.0" prefHeight="210.0" prefWidth="300.0">
         <children>
            <TextField fx:id="OldPasswordField" prefHeight="32.0" prefWidth="300.0" promptText="Enter Old Password" />
            <TextField fx:id="RetypeOldPasswordField" layoutY="45.0" prefHeight="32.0" prefWidth="300.0" promptText="Re-type Old Password" />
            <TextField fx:id="NewPasswordField" layoutY="134.0" prefHeight="32.0" prefWidth="300.0" promptText="Enter New Password" />
            <TextField fx:id="RetypeNewPasswordField" layoutY="178.0" prefHeight="32.0" prefWidth="300.0" promptText="Re-type New Password" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="30.0" layoutY="300.0" prefHeight="30.0" prefWidth="300.0">
         <children>
            <Button layoutY="-1.0" mnemonicParsing="false" onAction="#handleSave" prefHeight="30.0" prefWidth="140.0" style="-fx-background-color: LightSteelBlue;" text="Save">
               <font>
                  <Font name="Cambria Math" size="14.0" />
               </font></Button>
            <Button layoutX="160.0" layoutY="-1.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="30.0" prefWidth="140.0" style="-fx-background-color: LightSteelBlue;" text="Cancel">
               <font>
                  <Font name="Cambria Math" size="14.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
